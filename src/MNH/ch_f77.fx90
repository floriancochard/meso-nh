            STOP
         ENDIF         
         DO iw = 1, nw-1
            f(iw) = yg1(iw)
         ENDDO

      ELSEIF (msun .EQ. 4) THEN
         fil = 'DATAE1/SUN/nicolarv.flx'
         write(kout,*) fil
         OPEN(NEWUNIT=ilu,FILE=fil,STATUS='old')
         nhead = 8
         n = 1260
         DO i = 1, nhead
            READ(ilu,*)
         ENDDO
         DO i = 1, n
            READ(ilu,*) x1(i), y1(i)
         ENDDO
         CLOSE (ilu)
