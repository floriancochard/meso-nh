      PARAMETER(hc = 6.626068E-34 * 2.99792458E8)

* largest number of the machine:
      REAL largest
      PARAMETER(largest=1.E+36)

* small numbers (positive and negative)
      REAL pzero, nzero
      PARAMETER(pzero = +10./largest)
      PARAMETER(nzero = -10./largest)

* machine precision
	
      REAL precis
      PARAMETER(precis = 1.e-7)

* input

      INTEGER nw
      REAL wl(kw), wc(kw)
      
      INTEGER nz

      REAL tlev(kz)
      REAL airden(kz)

* weighting functions

      CHARACTER*50 jlabel(kj)
      INTEGER TPFLAG(kj)
      REAL sq(kj,kz,kw)

* input/output:
      INTEGER j

* data arrays

      INTEGER kdata
      PARAMETER(kdata=100)

